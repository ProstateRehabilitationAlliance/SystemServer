<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.prostate.base.dao.IpssDao">

	<select id="get" resultType="com.prostate.base.domain.IpssDO">
		select `id`,`parent_id`,`ipss_title`,`ipss_score`,`ipss_type`,`ipss_weight`,`order_weight`,`create_user`,`create_time`,`delete_user`,`delete_time`,`del_flag` from base_ipss where id = #{value}
	</select>

	<select id="list" resultType="com.prostate.base.domain.IpssDO">
		select `id`,`parent_id`,`ipss_title`,`ipss_score`,`ipss_type`,`ipss_weight`,`order_weight`,`create_user`,`create_time`,`delete_user`,`delete_time`,`del_flag` from base_ipss
        <where>  
		  		  <if test="id != null and id != ''"> and id = #{id} </if>
		  		  <if test="parentId != null and parentId != ''"> and parent_id = #{parentId} </if>
		  		  <if test="ipssTitle != null and ipssTitle != ''"> and ipss_title = #{ipssTitle} </if>
		  		  <if test="ipssScore != null and ipssScore != ''"> and ipss_score = #{ipssScore} </if>
		  		  <if test="ipssType != null and ipssType != ''"> and ipss_type = #{ipssType} </if>
		  		  <if test="ipssWeight != null and ipssWeight != ''"> and ipss_weight = #{ipssWeight} </if>
		  		  <if test="orderWeight != null and orderWeight != ''"> and order_weight = #{orderWeight} </if>
		  		  <if test="createUser != null and createUser != ''"> and create_user = #{createUser} </if>
		  		  <if test="createTime != null and createTime != ''"> and create_time = #{createTime} </if>
		  		  <if test="deleteUser != null and deleteUser != ''"> and delete_user = #{deleteUser} </if>
		  		  <if test="deleteTime != null and deleteTime != ''"> and delete_time = #{deleteTime} </if>
		  		  <if test="delFlag != null and delFlag != ''"> and del_flag = #{delFlag} </if>
		  		</where>
        <choose>
            <when test="sort != null and sort.trim() != ''">
                order by ${sort} ${order}
            </when>
			<otherwise>
                order by id desc
			</otherwise>
        </choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="count" resultType="int">
		select count(*) from base_ipss
		 <where>  
		  		  <if test="id != null and id != ''"> and id = #{id} </if>
		  		  <if test="parentId != null and parentId != ''"> and parent_id = #{parentId} </if>
		  		  <if test="ipssTitle != null and ipssTitle != ''"> and ipss_title = #{ipssTitle} </if>
		  		  <if test="ipssScore != null and ipssScore != ''"> and ipss_score = #{ipssScore} </if>
		  		  <if test="ipssType != null and ipssType != ''"> and ipss_type = #{ipssType} </if>
		  		  <if test="ipssWeight != null and ipssWeight != ''"> and ipss_weight = #{ipssWeight} </if>
		  		  <if test="orderWeight != null and orderWeight != ''"> and order_weight = #{orderWeight} </if>
		  		  <if test="createUser != null and createUser != ''"> and create_user = #{createUser} </if>
		  		  <if test="createTime != null and createTime != ''"> and create_time = #{createTime} </if>
		  		  <if test="deleteUser != null and deleteUser != ''"> and delete_user = #{deleteUser} </if>
		  		  <if test="deleteTime != null and deleteTime != ''"> and delete_time = #{deleteTime} </if>
		  		  <if test="delFlag != null and delFlag != ''"> and del_flag = #{delFlag} </if>
		  		</where>
	</select>
	 
	<insert id="save" parameterType="com.prostate.base.domain.IpssDO">
		insert into base_ipss
		(
			`id`, 
			`parent_id`, 
			`ipss_title`, 
			`ipss_score`, 
			`ipss_type`, 
			`ipss_weight`, 
			`order_weight`, 
			`create_user`, 
			`create_time`, 
			`delete_user`, 
			`delete_time`, 
			`del_flag`
		)
		values
		(
			#{id}, 
			#{parentId}, 
			#{ipssTitle}, 
			#{ipssScore}, 
			#{ipssType}, 
			#{ipssWeight}, 
			#{orderWeight}, 
			#{createUser}, 
			#{createTime}, 
			#{deleteUser}, 
			#{deleteTime}, 
			#{delFlag}
		)
	</insert>
	 
	<update id="update" parameterType="com.prostate.base.domain.IpssDO">
		update base_ipss 
		<set>
			<if test="parentId != null">`parent_id` = #{parentId}, </if>
			<if test="ipssTitle != null">`ipss_title` = #{ipssTitle}, </if>
			<if test="ipssScore != null">`ipss_score` = #{ipssScore}, </if>
			<if test="ipssType != null">`ipss_type` = #{ipssType}, </if>
			<if test="ipssWeight != null">`ipss_weight` = #{ipssWeight}, </if>
			<if test="orderWeight != null">`order_weight` = #{orderWeight}, </if>
			<if test="createUser != null">`create_user` = #{createUser}, </if>
			<if test="createTime != null">`create_time` = #{createTime}, </if>
			<if test="deleteUser != null">`delete_user` = #{deleteUser}, </if>
			<if test="deleteTime != null">`delete_time` = #{deleteTime}, </if>
			<if test="delFlag != null">`del_flag` = #{delFlag}</if>
		</set>
		where id = #{id}
	</update>
	
	<delete id="remove">
		delete from base_ipss where id = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from base_ipss where id in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>